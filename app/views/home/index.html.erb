            <a href="course-detail.html" class="educate-btn sm mx-xl-auto me-2">
              <span class="educate-btn__curve"></span>Buy Now
            </a>
          </div>
          <%= image_tag "vector-1.png", alt: "", class: "bottom_vector" %>
        </div>
      </div>
    <% end %>
  </div>
</section>
<!-- Courses Area End -->


